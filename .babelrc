{
  "presets": [
    "react-app",
    "es2015",
    "react",
    "stage-0",
  ],
  "plugins": [
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    [
      "module-resolver", {
        "alias": {
          "components":  "./src/components",
          "assets": "./src/assets",
          "utils": "./src/utils",
          "~": "./node_modules",
        },
      },
    ],
    [
      "import", {
        "libraryName": "antd",
        "style": "css",
      },
    ],
  ],
}
