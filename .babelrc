{
  "presets": [
    "react-app",
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "module-resolver", {
        "alias": {
          "components":  "./src/components",
        },
      },
    ],
    [
      "import", {
        "libraryName": "antd",
        "style": true,
      },
    ],
    "add-module-exports",
  ],
}
